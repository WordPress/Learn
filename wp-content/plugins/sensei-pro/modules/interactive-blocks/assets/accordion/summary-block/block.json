{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "sensei-lms/accordion-summary",
	"title": "Title",
	"parent": ["sensei-lms/accordion-section"],
	"description": "The accordion section title.",
	"category": "sensei-lms",
	"textdomain": "sensei-pro",
	"keywords": [],
	"attributes": {
		"summary": {
			"type": "string"
		},
		"level": {
			"type": "number",
			"default": 3
		},
		"lock": {
			"type": "object",
			"default": {
				"remove": true,
				"move": true
			}
		},
		"anchor": {
			"type": "string"
		}
	},
	"supports": {
		"sensei": {
			"frontend": true,
			"blockId": true
		},
		"lock": false,
		"html": false,
		"anchor": true,
		"color": {
			"background": true,
			"enableContrastChecker": true,
			"gradients": true,
			"text": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"blockGap": ["horizontal", "vertical"],
		"__experimentalBorder": true,
		"border": true,
		"alignWide": true,
		"typography": {
			"__experimentalFontFamily": true,
			"fontSize": true
		}
	}
}

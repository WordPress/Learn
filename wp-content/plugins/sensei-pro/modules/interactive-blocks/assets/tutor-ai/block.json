{
  "name": "sensei-pro/tutor-ai",
  "title": "Tutor AI",
  "category": "sensei-lms",
  "apiVersion": 2,
  "supports": {
    "align": true,
    "color": true,
    "sensei": {
      "blockId": true,
      "frontend": true,
      "required": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    }
  },
  "attributes": {
    "question": {
      "type": "string",
      "default": ""
    },
    "correctAnswer": {
      "type": "string",
      "default": ""
    },
    "reason": {
      "type": "string",
      "default": ""
    },
    "limit": {
      "type": "integer",
      "default": 5
    },
    "style": {
      "type": "object",
      "default": {
        "border": {
          "width": "1px",
          "radius": "12px",
          "color": "primary"
        },
        "spacing": {
          "padding": {
            "top": "20px",
            "right": "20px",
            "bottom": "20px",
            "left": "20px"
          }
        }
      }
    }
  },
  "textdomain": "sensei-pro"
}
